
@using System.Web
@using System.Web.Mvc

@{
    string usuarioRole = "";
    var role = HttpContext.Current.Session["UserRole"];
    var section = ViewBag.Section;

    switch (role)
    {
        case 1:
            usuarioRole = "Administrador";
            break;
        case 2:
            usuarioRole = "Mecanico";
            break;
        case 3:
            usuarioRole = "Usuario";
            break;
        default:
            break;
    }

}


