


@model AutoFixProyectoWeb.ModelDB.VEHICULOS_DE_CLIENTE_Result

@{
    ViewBag.Title = "ProyectoDelete_partial";
}

<!-- Modal Crear Servicio -->
<div class="modal modal-lg fade" id="eliminarVehiculo_@Model.PLACA" tabindex="-1" aria-labelledby="eliminarVehiculo"
     aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="agregarProyectoLabel">Eliminar Vehiculo</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                @using (Html.BeginForm("actualizarEstadoVehiculoController", "Cliente", FormMethod.Post))
                {
                    @Html.AntiForgeryToken()
                 
                    <input class="form-control" type="text" hidden value="@Model.PLACA" name="placa" required />

                    <input class="form-control" type="text" hidden value="@Model.MODELO" name="modelo" required />

                    <p class="text-danger">¿Esta seguro de eliminar el vehiculo?</p>
                    <p>Esta accion inabilitara al vehiculo y no podra usarlo en toda la aplicacion.</p>
                   
                    <div class="row mt-xl-3 justify-content-center text-center">
                        <div class="col-md-12">
                            <input type="submit" value="Eliminar" class="btn bg-gradient-dark">
                        </div>
                    </div>

                }
            </div>
        </div>
    </div>
</div>
