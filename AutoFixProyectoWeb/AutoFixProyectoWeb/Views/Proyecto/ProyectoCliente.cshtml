        <div class="container-fluid">
            <div class="page-header min-height-300 border-radius-xl mt-4"
                 style="background-image: url('../../Content/assets/img/curved-images/curved0.jpg'); background-position-y: 50%;">
                <span class="mask bg-gradient-primary opacity-6"></span>
            </div>
            <div class="card card-body blur shadow-blur mx-4 mt-n6 overflow-hidden">
                <div class="row gx-4">
                    <div class="col-auto">
                        <div class="avatar avatar-xl position-relative">
                            <img src="~/Content/assets/img/cars/Porche GT3 RS.jpeg" alt="profile_image"
                                 class="w-100 h-100 border-radius-lg shadow-sm" style="object-fit: cover;">
                        </div>
                    </div>
                    <div class="col-auto my-auto">
                        <div class="h-100">
                            <h5 class="mb-1">
                                Proyecto #@Model.idProyecto
                            </h5>
                            <p class="mb-0 font-weight-bold text-sm">
                                Status: <span class="badge badge-sm bg-gradient-success">@Model.estados[Model.estados.Count - 1].ESTADO</span>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="container-fluid py-4">
            <div class="row">
                @Html.Partial("Partials/ClienteProfile_partial", Model.perfil);
                @Html.Partial("Partials/EstadosList_partial", Model.estados);
                @Html.Partial("Partials/CostosList_partial");
            </div>
            <div class="card mb-4">
                <div class="card-header pb-0 p-3">
                    <h6 class="mb-1">Solicitudes Pendientes</h6>
                    <p class="text-sm">En esta sección se encontraran todos los carros en revisión</p>
                </div>
                <div class="card-body p-3">
                    <div class="row">


                        @foreach (var proyecto in Model.solicitudesPendientes)
                        {
                            <div class="col-xl-3 col-md-6 mb-xl-0 mb-4">
                                <div class="card card-blog card-plain">
                                    <div class="position-relative">
                                        <a class="d-block shadow-xl border-radius-xl">
                                            <img src="~/Content/assets/img/cars/Porche GT3 RS.jpeg" alt="img-blur-shadow"
                                                 class="img-fluid shadow border-radius-xl w-100 h-100" style="object-fit: cover;"
                                                 style="width: 500px;">
                                        </a>
                                    </div>
                                    <div class="card-body px-1 pb-0">
                                        <p class="text-gradient text-dark mb-2 text-sm">Project #@proyecto.ID_VEHICULO </p>
                                        <a href="javascript:;">
                                            <h5>
                                                @proyecto.NOMBRE_INVENTARIO
                                            </h5>
                                        </a>
                                        <p class="mb-4 text-sm">
                                            @proyecto.CANTIDAD
                                        </p>
                                        <div class="d-flex align-items-center justify-content-between">
