    <script>

        function aprobarSolicitud()
        {
            var modal = $('#aprobacionSolicitud');
            // modal.find('#txtPlaca').prop('readonly', true);
            modal.modal('show');

            var idSolicitud = modal.find('#txtSolicitud').val();
            

            $.ajax(
                {
                    url: "/Proyecto/actualizarEstadoSolicitudController",
                    type: "POST",
                    data: {
                        "id": idSolicitud,
                        "estadoSolicitud": 2
                    },

                    dataType: "json",
                    success: function (data) {
                        $('#aprobacionSolicitud').modal('hide');
                       // document.getElementById('errorAlert').style.display = "none";

                    },
                    error: function (xhr, status, error) {
                        // Handle errors here
                        //document.getElementById('errorAlert').style.display = "";
                    }
                })

            
        }

        function rechazarSolicitud()
        {
            var modal = $('#aprobacionSolicitud');
            // modal.find('#txtPlaca').prop('readonly', true);
            modal.modal('show');

            var idSolicitud = modal.find('#txtSolicitud').val();

            $.ajax(
                {
                    url: "/Proyecto/actualizarEstadoSolicitudController",
                    type: "POST",
                    data: {
                        "id": idSolicitud,
                        "estadoSolicitud": 1
                    },

                    dataType: "json",
                    success: function (data) {
                        //document.getElementById('errorAlert').style.display = "none";
                        $('#aprobacionSolicitud').modal('hide');
                    },
                    error: function (xhr, status, error) {
                        // Handle errors here
                        //document.getElementById('errorAlert').style.display = "";
                    }
                })
        }

        $(document).ready(function () {
            $('.ver-solicitud').on('click', function (event) {
                var idSolicitud = $(this).data('id');
                console.log("idSolicitud", idSolicitud);

                $.ajax(
                    {
                        url: "/Proyecto/getSolicitudByIDController",
                        type: "POST",
                        dataType: "json",
                        data: {
                            "id": idSolicitud
                        },
                        success: function (data) {

                            console.log(data);
                            var modal = $('#aprobacionSolicitud');
                            // modal.find('#txtPlaca').prop('readonly', true);
                            modal.modal('show');

                            modal.find('#txtSolicitud').val(data.ID_SOLICITUD);
                            modal.find('#txtProyecto').val(data.ID_VEHICULO);
                            modal.find('#txtPieza').val(data.NOMBRE_INVENTARIO);
                            modal.find('#txtCantidad').val(data.CANTIDAD);

                        },
                        error: function (xhr, status, error) {
                            console.log(error);
                            // Handle errors here
                            //document.getElementById('errorAlert').style.display = "";
                        }
                    })

            });

            $('.ver-solicitudHistorial').on('click', function (event) {
                var idSolicitud = $(this).data('id');
                console.log("idSolicitud", idSolicitud);

                $.ajax(
                    {
                        url: "/Proyecto/getSolicitudByIDController",
                        type: "POST",
                        dataType: "json",
                        data: {
                            "id": idSolicitud
                        },
                        success: function (data) {

                            console.log(data);
                            var modal = $('#verSolicitud');
                            // modal.find('#txtPlaca').prop('readonly', true);
                            modal.modal('show');

                            modal.find('#txtSolicitud').val(data.ID_SOLICITUD);
                            modal.find('#txtProyecto').val(data.ID_VEHICULO);
                            modal.find('#txtPieza').val(data.NOMBRE_INVENTARIO);
                            modal.find('#txtCantidad').val(data.CANTIDAD);
                            modal.find('#txtEstado').val(data.ESTADO_DESCRIPCION);

                        },
                        error: function (xhr, status, error) {
                            console.log(error);
                            // Handle errors here
                            //document.getElementById('errorAlert').style.display = "";
                        }
                    })

            });
        });

